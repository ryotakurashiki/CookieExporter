{
  "manifest_version": 3,
  "name": "Cookie Exporter",
  "version": "1.0",
  "permissions": [
    "cookies",
    "activeTab",
    "scripting",
    "downloads",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}